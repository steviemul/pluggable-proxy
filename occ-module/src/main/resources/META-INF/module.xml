<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<module>
  <name>Oracle Commerce</name>
  <namespace>stevemul.net.occ</namespace>
  <loading-priority>90</loading-priority>
   <settings>
  	<setting>
  	  <name>servingNonMin</name>
  	  <type>CHECKBOX</type>
  	  <order>1</order>
  	  <label>Serve Non Minified Element, Widget &amp; Application Level JS.</label>
  	</setting>
  	<setting>
  	  <name>overrideMain</name>
  	  <type>CHECKBOX</type>
  	  <order>2</order>
  	  <label>Override main.js with a local copy.</label>
  	</setting>
  	<setting>
  	  <name>overrideMainLocation</name>
  	  <type>FILE</type>
  	  <order>3</order>
  	  <label>Location of local main.js.</label>
  	</setting>
  	<setting>
  	  <name>overrideStorelibs</name>
  	  <type>CHECKBOX</type>
  	  <order>4</order>
  	  <label>Override store-libs.js with a local copy.</label>
  	</setting>
  	<setting>
  	  <name>overrideStoreLibsLocation</name>
  	  <type>FILE</type>
  	  <order>5</order>
  	  <label>Location of local store-libs.js</label>
  	</setting>
  	<setting>
  	  <name>templateDirectory</name>
  	  <type>FOLDER</type>
  	  <order>6</order>
  	  <label>Template Directory</label>
  	</setting>
  	<setting>
  	  <name>dumpTemplates</name>
  	  <type>CHECKBOX</type>
  	  <order>7</order>
  	  <label>Dump Templates</label>
  	</setting>
  	<setting>
  	  <name>allowTemplateOverrides</name>
  	  <type>CHECKBOX</type>
  	  <order>8</order>
  	  <default>false</default>
  	  <label>Allow Template Overrides</label>
  	</setting>
  </settings>
  <request-processors>
  	<processor>net.stevemul.proxy.processors.TrafficRewriteRequestProcessor</processor>
  	<processor>net.stevemul.proxy.processors.JavascriptLibsOverrideRequestProcessor</processor>
  </request-processors>
  <response-processors>
  	<processor>net.stevemul.proxy.processors.PagesEndpointResponseProcessor</processor>
  </response-processors>
</module>